<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.issband.Controller.DeleteTicketController">
    <children>
        <Label layoutX="20.0" layoutY="31.0" text="Audience Name:" />
        <TextField fx:id="audienceNameTextField" layoutX="130.0" layoutY="27.0" promptText="Enter Audience Name" />
        <Button fx:id="deleteTicketButton" layoutX="150.0" layoutY="90.0" mnemonicParsing="false" onAction="#handleDeleteTicket" text="Delete Ticket" />
    </children>
</AnchorPane>
