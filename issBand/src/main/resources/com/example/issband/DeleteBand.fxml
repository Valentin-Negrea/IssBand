<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.issband.Controller.DeleteBandController">
    <children>
        <Label layoutX="20.0" layoutY="20.0" text="Enter Band Name:" />
        <TextField fx:id="bandNameTextField" layoutX="150.0" layoutY="20.0" prefWidth="200.0" />
        <Button fx:id="deleteButton" layoutX="150.0" layoutY="60.0" mnemonicParsing="false" onAction="#handleDeleteBand" text="Delete Band" />
    </children>
</AnchorPane>
